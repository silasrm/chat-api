{
    "name": "silasrm/chat-api",
    "description": "Chat API Notifications channel for Laravel 5.6+",
    "homepage": "https://github.com/silasrm/chat-api",
    "license": "MIT",
    "keywords": [
        "laravel",
        "notifications",
        "notification-channel",
        "chat-api",
        "whatsapp",
        "chat"
    ],
    "authors": [
        {
            "name": "Silas Ribas",
            "email": "silasrm@gmail.com",
            "homepage": "https://silasribas.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.0",
        "mikechip/chatapi": "2.0.*",
        "illuminate/notifications": "^5.6|^6.0|^7.0|^8.0",
        "illuminate/queue": "^5.6|^6.0|^7.0|^8.0",
        "illuminate/support": "^5.6|^6.0|^7.0|^8.0",
        "ext-json": "*",
        "ext-fileinfo": "*"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Silasrm\\ChatApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Silasrm\\ChatApi\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Silasrm\\ChatApi\\ChatApiServiceProvider"
            ]
        }
    }
}
